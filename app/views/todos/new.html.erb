<h1>New todo</h1>

<%= form_for(@todo) do |f|  %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%# <%= f.input :deadline, as: :string, label: "Dates", placeholder: "Deadline", wrapper_html: { class: "inline_field_wrapper" } %> %>
  <%= f.submit %>
<% end %>

<%= link_to 'Back', todos_path %>
